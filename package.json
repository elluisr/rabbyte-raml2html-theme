{
  "name": "raml2html-rabbyte-theme",
  "version": "1.0.0",
  "description": "Rabbyte theme for raml2html utility.",
  "main": "index.js",
  "scripts": {
    "build": "cat ./scss/main.scss | node-sass > ./dist/style.css",
    "test": "raml2html -v --template ./index.nunjucks -i ./demo/raml-examples/others/world-music-api/api.raml -o ./demo/api.html",
    "test:w": "watch -p \"/dist/\" -p \"/demo/\" \"npm run build-sass && npm run test\" ./",
    "prepublish": "npm run build"
  },
  "author": {
    "name": "Luis Rodrigues",
    "email": "luis.ms.rodrigues@hotmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elluisr/raml2html-rabbyte-theme.git"
  },
  "homepage": "https://github.com/elluisr/raml2html-rabbyte-theme#readme",
  "keywords": [
    "raml",
    "raml2html",
    "modern",
    "design",
    "theme"
  ],
  "devDependencies": {
    "node-sass": "^4.11.0",
    "watch": "^1.0.2",
    "raml2html": "^7.2.0"
  }
}
